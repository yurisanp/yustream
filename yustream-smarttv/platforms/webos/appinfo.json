{
  "id": "com.yustream.tv",
  "version": "1.0.0",
  "vendor": "YuStream",
  "type": "web",
  "main": "index.html",
  "title": "YuStream TV",
  "icon": "icon.png",
  "largeIcon": "icon-large.png",
  "splashBackground": "splash.png",
  "transparent": false,
  "resolution": "1920x1080",
  "disableBackHistoryAPI": true,
  "usePrerendering": true,
  "handlesRelaunch": true,
  "requestedWindowOrientation": "landscape",
  "supportGlobalSearch": false,
  "category": {
    "primary": "multimedia",
    "secondary": "streaming"
  },
  "bgImage": "background.png",
  "iconColor": "#000000",
  "tileColor": "#007AFF",
  "requiredPermissions": [
    "network.operation",
    "media.operation",
    "tv.operation"
  ],
  "trustLevel": "default",
  "allowVideoCapture": false,
  "allowAudioCapture": false,
  "allowCrossDomain": true,
  "enableNodeJS": false,
  "uiRevision": "2",
  "accessibility": {
    "supportsAudioGuidance": false,
    "supportsHighContrast": false
  },
  "mediaCapabilities": {
    "video": {
      "codecs": ["h264", "h265"],
      "containers": ["mp4", "hls"]
    },
    "audio": {
      "codecs": ["aac", "mp3"],
      "containers": ["mp4", "hls"]
    }
  },
  "displayAffinity": "main",
  "supportQuickStart": true,
  "supportDeepLink": true,
  "keywords": ["streaming", "video", "yustream", "tv"],
  "age": "12+",
  "supportMultipleWindows": false,
  "v8SnapshotFile": "",
  "v8StartupSnapshot": false,
  "hardwareFeaturesNeeded": [
    "network"
  ]
}
