<?xml version="1.0" encoding="UTF-8"?>
<Server version="8">
    <Name>YustreamOME</Name>
    <Type>origin</Type>
    <IP>*</IP>
    <PrivacyProtection>false</PrivacyProtection>
    <StunServer>stun.l.google.com:19302</StunServer>
    
    <Modules>
        <ERTMP>
            <Enable>true</Enable>
        </ERTMP>
    </Modules>
    <!-- Configurações de porta -->
    <Bind>
        <Managers>
			<API>
				<Port>8081</Port>
			</API>
		</Managers>
        <Providers>
            <RTMP>
                <Port>1935</Port>
                <WorkerCount>1</WorkerCount>
            </RTMP>
            <WebRTC>
                <Signalling>
                    <Port>3333</Port>
                    <TLSPort>3334</TLSPort>
                </Signalling>
                <IceCandidates>
                    <TcpRelay>127.0.0.1:3478</TcpRelay>
                    <TcpForce>false</TcpForce>
                    <IceCandidate>127.0.0.1:10000-10009/udp</IceCandidate>
                </IceCandidates>
            </WebRTC>
        </Providers>
        <Publishers>
            <LLHLS>
                <Port>8080</Port>
                <TLSPort>8443</TLSPort>
                <WorkerCount>2</WorkerCount>
            </LLHLS>
            <WebRTC>
                <Signalling>
                    <Port>3333</Port>
                    <TLSPort>3334</TLSPort>
                    <WorkerCount>1</WorkerCount>
                </Signalling>
                <IceCandidates>
                    <TcpRelay>127.0.0.1:3478</TcpRelay>
                    <IceCandidate>127.0.0.1:10000-10009/udp</IceCandidate>
                </IceCandidates>
            </WebRTC>
        </Publishers>
    </Bind>

    <Managers>
		<API>
			<AccessToken>maketears</AccessToken>
			<CrossDomains>
				<Url>*</Url>
			</CrossDomains>
		</API>
	</Managers>

    <VirtualHosts>
        <VirtualHost include="VHost.xml" />
    </VirtualHosts>
</Server>