<?xml version="1.0" encoding="UTF-8"?>
<Server version="8">
    <Name>YustreamOME</Name>
    <Type>origin</Type>
    <IP>*</IP>
    <PrivacyProtection>true</PrivacyProtection>
    <StunServer>stun.l.google.com:19302</StunServer>
    
    <Modules>
        <ERTMP>
            <Enable>true</Enable>
        </ERTMP>
    </Modules>
    <!-- Configurações de porta -->
    <Bind>
        <Managers>
			<API>
				<Port>8081</Port>
			</API>
		</Managers>
        <Providers>
            <RTMP>
                <Port>1935</Port>
                <WorkerCount>4</WorkerCount>
            </RTMP>
        </Providers>
        <Publishers>
            <LLHLS>
                <Port>8080</Port>
                <TLSPort>8443</TLSPort>
                <WorkerCount>4</WorkerCount>
            </LLHLS>
        </Publishers>
    </Bind>

    <Managers>
		<API>
			<AccessToken>maketears</AccessToken>
			<CrossDomains>
				<Url>*</Url>
			</CrossDomains>
		</API>
	</Managers>

    <VirtualHosts>
        <VirtualHost include="VHost.xml" />
    </VirtualHosts>
</Server>